{
  "name": "sunsharing-component",
  "version": "1.0.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "start": "concurrently \"sharekit server\" \"npm run build-component-dev\"",
    "build": "sharekit build",
    "build-analyze": "sharekit build --analyze",
    "build-component-dev": "rimraf src/lib && rimraf src/es && rollup -c ./rollup.config.dev.js --watch",
    "build-component-prod": "rimraf visualizer && rimraf src/lib && rimraf src/es && rimraf lib && rimraf es && rollup -c ./rollup.config.prod.js",
    "lint": "eslint --ext .js src > eslintErrorLog.txt"
  },
  "devDependencies": {
    "@yo/eslint-config-sunsharing": "^1.2.2",
    "@yo/share-kit": "^0.5.1",
    "autoprefixer": "^8.5.0",
    "babel-plugin-dva-hmr": "^0.3.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.5.1",
    "cssnano": "^3.10.0",
    "less": "^3.0.4",
    "node-sass": "^4.9.0",
    "redbox-react": "^1.5.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.59.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-image-base64": "^1.0.0",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.1",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "3.0.0",
    "rollup-plugin-visualizer": "^0.6.0",
    "rollup-watch": "^4.3.1"
  },
  "dependencies": {
    "antd": "^3.2.3",
    "babel-plugin-import": "^1.6.6",
    "classnames": "^2.2.5",
    "dva": "^2.1.0",
    "nprogress": "^0.2.0",
    "rc-form": "^2.1.7",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
